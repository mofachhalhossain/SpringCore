<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
<!--  this is bean -->

<bean class="com.springcore.autowire.Address" name="address">
<property name="street" value="street 1" />
<property name="city" value="city y" />
</bean>

<bean class="com.springcore.autowire.Emp" name="emp1" autowire="byType"> <!-- Autowire by 1.Name, 2.Type, 3.constructor -->

</bean>

        
</beans>